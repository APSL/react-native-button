  "author": "Alvaro Medina Ballester <me@alvaromb.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/APSL/react-native-button/issues"
  },
  "homepage": "https://github.com/APSL/react-native-button#readme",
  "dependencies": {
    "lodash.isequal": "^4.1.4",
    "prop-types": "^15.5.10"

  "devDependencies": {
    "babel-eslint": "^6.0.5",
    "babel-jest": "^15.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-react-native": "^1.9.0",
    "es6-promise": "^3.3.1",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-react-native": "^1.1.0-beta",
    "jest": "^20.0.0",
    "jest-react-native": "^18.0.0",
    "react": "^15.4.0",
    "react-native": "^0.50.0",
    "react-test-renderer": "^15.4.0",
    "whatwg-fetch": "^1.0.0"
  }
